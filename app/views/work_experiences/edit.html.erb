<h1>Edit work experience </h1>

<%= form_for [@user, @experience] do |f| %>
    <%= f.label "Company"%>
    <%= f.text_field :employer %><br>

    <%= f.label "Job title"%>
    <%= f.text_field :title %><br>
    
    <%= f.label "Location"%>
    <%= f.text_field :location %><br>

    <%= f.label "Start date"%>
    <%= f.text_field :start_date %><br>

    <%= f.label "End date"%>
    <%= f.text_field :end_date %><br>

    <%= f.label "Experience"%>
    <%= f.text_field :experience %><br>

    <%= f.label "Ranking"%>
    <%= f.number_field :ranking %><br>

    <%= f.submit "Update details"%>
<% end %>